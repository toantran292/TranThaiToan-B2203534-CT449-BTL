{
  "name": "server",
  "version": "1.0.0",
  "license": "MIT",
  "main": "server.ts",
  "scripts": {
    "dev": "NODE_ENV=developments npx tsx --watch src/server.ts",
    "dev2": "NODE_ENV=developments nodemon --watch './src/**/*.ts' --ext ts --exec 'rimraf ./dist && tsc && tsc-alias && node dist/server.js'",
    "dev:build": "npm run build; npm run start",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "build:dev": "rimraf ./dist && tsc --watch && tsc-alias",
    "start": "NODE_ENV=prod node dist/server.js"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "fast-safe-stringify": "^2.1.1",
    "http-status-codes": "^2.3.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.2.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.25",
    "@typescript-eslint/parser": "^7.3.1",
    "nodemon": "^3.1.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  }
}
